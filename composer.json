{
    "name": "laravel_payment/telr",
    "description": "Pay online through telr payment gateway",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "payment",
        "telr payment",
        "telr"
    ],
    "authors": [
        {
            "name": "Hridoy Mahmud",
            "email": "hridoymahmud71@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TelrGateway\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TelrGateway\\TelrServiceProvider"
